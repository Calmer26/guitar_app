    </score-part>
    <!-- Piano -->
    <score-part id="Piano">
      <part-name>Piano</part-name>
      <score-instrument id="Piano-Instr">
        <instrument-name>Acoustic Grand Piano</instrument-name>
      </score-instrument>
      <midi-instrument id="Piano-Instr">
        <midi-channel>2</midi-channel>
